  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "typecheck:pretty": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "jest --silent",
    "test:watch": "npm run test --watch --silent",
    "test:update-snapshot": "npm run test --updateSnapshot --silent",
    "test:coverage": "npm run test -- --coverage",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install && chmod ug+x .husky/*",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:storybook": "storybook build",
    "doctoc": "doctoc --title='## Table of Contents' --github README.md && npm run format",
    "predoctoc:check": "cp README.md v-README.md.orig && npm run doctoc",
    "doctoc:check": "diff -q README.md v-README.md.orig",
    "postdoctoc:check": "mv v-README.md.orig README.md",
    "git:clean:dev": "sh ./scripts/git-clean.sh dev",
    "git:clean:stage": "sh ./scripts/git-clean.sh stage"
  },
